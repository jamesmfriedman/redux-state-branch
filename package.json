    "@types/react-redux": "^6.0.6",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "flowgen": "^1.2.2",
    "prettier-tslint": "^0.4.0",
    "react-app-rewired": "^1.5.2",
    "typescript": "^3.0.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.17.0"
