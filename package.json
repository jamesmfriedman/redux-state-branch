{
  "name": "redux-state-branch",
  "version": "0.0.3",
  "private": false,
  "description": "A redux wrapper for sane people.",
  "homepage": "https://jamesmfriedman.github.io/redux-state-branch/",
  "main": "dist/index",
  "peerDependencies": {
    "redux": "^4.0.0"
  },
  "scripts": {
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "build": "./scripts/build.sh",
    "build:docs": "rm -R -f docs && react-app-rewired build --scripts-version react-scripts-ts && mv build docs",
    "test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "version": "npm run build",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^9.6.26",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-test-renderer": "^16.0.2",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "flowgen": "^1.2.2",
    "normalize.css": "^8.0.0",
    "prettier-tslint": "^0.4.0",
    "prismjs": "^1.15.0",
    "react": "^16.4.2",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.4.2",
    "react-markdown-loader": "git+https://github.com/jamesmfriedman/react-markdown-loader.git",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "rmwc": "^1.9.2",
    "typescript": "^3.0.1"
  }
}
